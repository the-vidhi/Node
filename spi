cond split 
up - !ISNULL([ID (1)]) && !ISNULL(ID)
ins - ISNULL([ID (1)]) && !ISNULL(ID)
del - !ISNULL([ID (1)]) && ISNULL(ID)


cond split = update
(Name != [Name (1)]) || (Address != [Address (1)]) || (City != [City (1)]) || (State != [State (1)]) || (Country != [Country (1)])

update
update FinalTB
set Name = ?,
Address = ?
where ID = ?

delete from FinalTB
 where ID = ?


System.Windows.Forms.MessageBox.Show("Updated Records: " + Dts.Variables[0].Value.ToString());
